{
    "name": "@navikt/nav-dekoratoren-moduler",
    "version": "1.9.0",
    "description": "Moduler til nav-dekoratoren",
    "author": "NAVIKT",
    "license": "MIT",
    "repository": "navikt/nav-dekoratoren-moduler",
    "main": "csr/index.js",
    "module": "csr/index.js",
    "source": "src/csr/index.tsx",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "npm-run-all --parallel start:*",
        "build": "npm-run-all --parallel build:*",
        "test": "jest",
        "start:csr": "rollup -c rollup.config.csr.js --bundleConfigAsCjs -w",
        "start:ssr": "rollup -c rollup.config.ssr.js --bundleConfigAsCjs -w",
        "build:csr": "rollup -c rollup.config.csr.js",
        "build:ssr": "rollup -c rollup.config.ssr.js",
        "predeploy": "cd example && npm install && npm run build",
        "publish:beta": "npm run build && npm version prerelease --preid beta && npm publish --access public --tag beta"
    },
    "dependencies": {
        "csp-header": "^5.1.0",
        "html-react-parser": "3.0.12",
        "jsdom": "^16.6.0",
        "node-cache": "^5.1.2",
        "prop-types": "^15.7.2",
        "react": "18.2.0"
    },
    "devDependencies": {
        "@rollup/plugin-terser": "0.1.0",
        "@types/jest": "29.2.1",
        "@types/jsdom": "16.2.13",
        "@types/node": "16.18.3",
        "@types/node-cache": "4.2.5",
        "@types/node-fetch": "2.5.11",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "@types/react-html-parser": "2.0.2",
        "html-react-parser": "3.0.12",
        "jest": "29.2.2",
        "jest-fetch-mock": "3.0.3",
        "jest-watch-typeahead": "2.2.0",
        "jsdom": "16.6.0",
        "node-fetch": "3.3.1",
        "npm-run-all": "4.1.5",
        "prettier": "2.7.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-scripts": "5.0.1",
        "rollup": "3.2.5",
        "rollup-plugin-ts": "3.0.2",
        "ts-jest": "29.0.3",
        "tslib": "2.4.1",
        "typescript": "4.8.4"
    },
    "files": [
        "csr",
        "ssr"
    ],
    "browser": {
        "child_process": false
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    }
}
